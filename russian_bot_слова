meme_dict = {
            "кринж": "Что-то очень странное или стыдное",
            "лол": "Что-то очень смешное",
            "рофл": "шутка",
            "агриться": "злиться",
            "тг": "телеграмм",
            "тгк": "телеграм канал"
            }
word = input("Введите непонятное слово: ")
if word in meme_dict.keys():
    print(meme_dict[word])
else:
    print("такого слова не нашлось")
